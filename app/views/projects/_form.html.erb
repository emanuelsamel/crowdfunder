<%= form_for(@project, html: {multipart: true}) do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <br>
  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <br>
  <div>
    <%= f.label :goal %>
    <%= f.number_field :goal %>
  </div>
  <br>
  <div>
    <%= f.label :image %>
    <div class="file-button">
      <%= f.file_field :image %>
    </div>
  </div>
  <br>
  <div>
    <%= f.label :start_date %>
    <%= f.datetime_select :start_date %>
  </div>
  <br>
  <div>
    <%= f.label :end_date %>
    <%= f.datetime_select :end_date %>
  </div>
  <br>
  <%= f.submit %>
  <br>
<% end %>
